<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.business.base.mapper.VOutPutInfoMapper">
    
    <resultMap type="VOutPutInfo" id="VOutPutInfoResult">
        <result property="id"    column="id"    />
        <result property="outPutCode"    column="out_put_code"    />
        <result property="outPutName"    column="out_put_name"    />
        <result property="entCode"    column="ent_code"    />
        <result property="entName"    column="ent_name"    />
        <result property="mnNum"    column="mn_num"    />
        <result property="latitude"    column="latitude"    />
        <result property="longitude"    column="longitude"    />
        <result property="monitoringPointType"    column="monitoring_point_type"    />
        <result property="pollutantName"    column="pollutant_name"    />
        <result property="createTime"    column="create_time"    />
    </resultMap>

    <sql id="selectVOutPutInfoVo">
        select id, out_put_code, out_put_name, ent_code, ent_name, mn_num, latitude, longitude, monitoring_point_type, pollutant_name, create_time from v_out_put_info
    </sql>

    <select id="selectVOutPutInfoList" parameterType="VOutPutInfo" resultMap="VOutPutInfoResult">
        <include refid="selectVOutPutInfoVo"/>
        <where>  
            <if test="outPutCode != null  and outPutCode != ''"> and out_put_code = #{outPutCode}</if>
            <if test="outPutName != null  and outPutName != ''"> and out_put_name like concat('%', #{outPutName}, '%')</if>
            <if test="entCode != null  and entCode != ''"> and ent_code = #{entCode}</if>
            <if test="entName != null  and entName != ''"> and ent_name like concat('%', #{entName}, '%')</if>
            <if test="mnNum != null  and mnNum != ''"> and mn_num = #{mnNum}</if>
            <if test="latitude != null  and latitude != ''"> and latitude = #{latitude}</if>
            <if test="longitude != null  and longitude != ''"> and longitude = #{longitude}</if>
            <if test="monitoringPointType != null  and monitoringPointType != ''"> and monitoring_point_type = #{monitoringPointType}</if>
            <if test="pollutantName != null  and pollutantName != ''"> and pollutant_name like concat('%', #{pollutantName}, '%')</if>
            <!-- 根据所属企业数据范围过滤 -->
            <if test="params.dataEntScope != null and params.dataEntScope != ''">
                ${params.dataEntScope}
            </if>
        </where>
    </select>
    
    <select id="selectVOutPutInfoById" parameterType="Long" resultMap="VOutPutInfoResult">
        <include refid="selectVOutPutInfoVo"/>
        where id = #{id}
    </select>

    <insert id="insertVOutPutInfo" parameterType="VOutPutInfo">
        insert into v_out_put_info
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="outPutCode != null">out_put_code,</if>
            <if test="outPutName != null">out_put_name,</if>
            <if test="entCode != null">ent_code,</if>
            <if test="entName != null">ent_name,</if>
            <if test="mnNum != null">mn_num,</if>
            <if test="latitude != null">latitude,</if>
            <if test="longitude != null">longitude,</if>
            <if test="monitoringPointType != null">monitoring_point_type,</if>
            <if test="pollutantName != null">pollutant_name,</if>
            <if test="createTime != null">create_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="outPutCode != null">#{outPutCode},</if>
            <if test="outPutName != null">#{outPutName},</if>
            <if test="entCode != null">#{entCode},</if>
            <if test="entName != null">#{entName},</if>
            <if test="mnNum != null">#{mnNum},</if>
            <if test="latitude != null">#{latitude},</if>
            <if test="longitude != null">#{longitude},</if>
            <if test="monitoringPointType != null">#{monitoringPointType},</if>
            <if test="pollutantName != null">#{pollutantName},</if>
            <if test="createTime != null">#{createTime},</if>
         </trim>
    </insert>

    <update id="updateVOutPutInfo" parameterType="VOutPutInfo">
        update v_out_put_info
        <trim prefix="SET" suffixOverrides=",">
            <if test="outPutCode != null">out_put_code = #{outPutCode},</if>
            <if test="outPutName != null">out_put_name = #{outPutName},</if>
            <if test="entCode != null">ent_code = #{entCode},</if>
            <if test="entName != null">ent_name = #{entName},</if>
            <if test="mnNum != null">mn_num = #{mnNum},</if>
            <if test="latitude != null">latitude = #{latitude},</if>
            <if test="longitude != null">longitude = #{longitude},</if>
            <if test="monitoringPointType != null">monitoring_point_type = #{monitoringPointType},</if>
            <if test="pollutantName != null">pollutant_name = #{pollutantName},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteVOutPutInfoById" parameterType="Long">
        delete from v_out_put_info where id = #{id}
    </delete>

    <delete id="deleteVOutPutInfoByIds" parameterType="String">
        delete from v_out_put_info where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>