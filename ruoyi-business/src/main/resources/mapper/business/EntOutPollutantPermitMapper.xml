    <select id="selectEntOutPollutantPermitList" parameterType="com.ruoyi.business.envProt.domain.EntOutPollutantPermitReq"
            resultMap="entOutPollutantPermitResult">
        select
            p.poll_permit_id, p.ent_code, e.ent_name, p.cert_unit_name, p.cert_unit_code, p.permit_level, p.permit_num, p.begin_date, p.end_date,
            p.issue_office, p.issue_date, p.report_require, p.product_ids, p.product_output, p.account, p.password, p.gas_poll_type, p.gas_emission_rule,
            p.gas_execute_standard, p.water_poll_type, p.water_emission_rule, p.water_execute_standard, p.create_user, p.create_time, p.update_user,
            p.update_time, p.remark
        from t_bas_ent_out_pollutant_permit p
        left join t_bas_enterprise e on e.ent_code = p.ent_code
        <where>  
            <if test="entCode != null and entCode != ''"> and p.ent_code = #{entCode}</if>
            <if test="entName != null and entName != ''"> and e.ent_name like concat('%', #{entName}, '%')</if>
            <if test="certUnitName != null and certUnitName != ''"> and p.cert_unit_name like concat('%', #{certUnitName}, '%')</if>
            <if test="permitNum != null and permitNum != ''"> and p.permit_num like concat('%', #{permitNum}, '%')</if>
            <if test="entCodes != null and entCodes.size() > 0"> and p.ent_code in
                <foreach item="code" collection="entCodes" open="(" separator="," close=")">
                    #{code}
                </foreach>
            </if>
        </where>
    </select>

